{
    "height": 30,
    "spacing": 4,
    "modules-left": [
        "autohide"
    ],
    "modules-center": [
        "clock"
    ],
    "modules-right": [
        "battery",
        "network"
    ],
    "autohide": {
        "threshold-hidden-y": 1,
        "threshold-visible-y": 50,
        "delay-hide": 1000,
        "delay-show": 0,
        "check-interval": 100,
        "consecutive-checks-before-visible": 3
    },
    "clock": {
        "format": "{:%H:%M:%S}"
    }
}
